/*
  X-Frame-Options: DENY
  X-XSS-Protection: 1; mode=block
  X-Content-Type-Options: nosniff
  Referrer-Policy: strict-origin-when-cross-origin
  Content-Security-Policy-Report-Only: default-src 'self'; font-src https://themes.googleusercontent.com; img-src 'self'; https: 'unsafe-inline' 'unsafe-eval'

  Link: <styles/styles.css>; rel=preload; as=stylesheet, <images/avatar.png>; rel=preload; as=image
